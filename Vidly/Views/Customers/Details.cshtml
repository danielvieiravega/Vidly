@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

        <ul class="list-group">
           
                  @if (Model.Birthday != null)
                  {
                      <li class="list-group-item"> @($"{@Model.Birthday:M/d/yyyy}")</li>

                  }
            <li class="list-group-item"> Membership Type: @Model.MembershipType.Name </li>
        </ul>
